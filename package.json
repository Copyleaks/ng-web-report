    "rxjs": "~6.6.7",
    "scroll-into-view-if-needed": "^2.2.28",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.0",
    "@angular/cli": "~13.3.0",
